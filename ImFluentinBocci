ProtocolsDict = {'FTP': '21', 'DNS': ' 53', 'LDAP': '389', 'MySQL': '3306'}
question = input("For which protocol would you like to know the port number?")

#For Protocol Discovery, input value in the output section in the order of Name: to yield :Number

if question in ProtocolsDict.keys():
    answer = ProtocolsDict[question]
    print("I am C-3PO, Human-Cyborg Relations! The port number for protocol " + question + " is " + answer + "!")

else:
    print("Oh bother...the protocol can't be found.")
